target 'Beacon' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'GoogleMaps'
  pod 'Parse'
  pod 'Async'
  pod 'MGSwipeTableCell'
  pod 'XLActionController'
  pod 'CZPicker'

  # Pods for Beacon

  target 'BeaconTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BeaconUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
